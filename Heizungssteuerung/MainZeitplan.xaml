<Window x:Class="Heizungssteuerung.MainZeitplan"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:Heizungssteuerung.UIElemente"
        Title="Zeitplanübersicht" Height="700" Width="500" x:Name="_this">
    <Grid>
        <local:ZeitplanUiElement ZeitplanElement="{Binding Path=ZeitplanElementMain, ElementName=_this, Mode=TwoWay}" Height="50" Margin="0,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Background="GhostWhite"/>
    </Grid>
</Window>
