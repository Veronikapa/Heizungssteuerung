<Window x:Class="Heizungssteuerung.MainZeitplan"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:Heizungssteuerung.UIElemente"
        xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
        xmlns:h="clr-namespace:Heizungssteuerung"
        Title="Zeitplanübersicht" Height="700" Width="500" x:Name="_this" Background="White">
    <Grid>
        <StackPanel Background="White" Height="25" VerticalAlignment="Top">
            <Button Height="25" Width="30" HorizontalAlignment="Left" Content="Add" PreviewMouseDown="Neu_MouseDown"/>
        </StackPanel>
        <StackPanel x:Name="ZeitplanPanel" Margin="0,25,0,0">
        
        </StackPanel>
    </Grid>
  
</Window>
